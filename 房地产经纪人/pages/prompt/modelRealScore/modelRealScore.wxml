<!--pages/prompt/modelRealScore/modelRealScore.wxml-->
<view class="mycontainer">
  <view class="upper">
    <view class="upper-in">
      <view class ="time">
        <image src="/imgs/time.png" mode="aspectFit" class = "time-img"></image>
        <text class = "time-text">用时 00：15</text>
      </view>
      <view class = "score">
        <text class ="score-top">2</text>
        <text class ="score-bottom">不合格</text>
      </view>
      <view class = "line">
        <view class = "line-left"></view>
        <view class = "line-middle" >
          <view class ="line-middle-in"></view>
        </view>
        <view class = "line-right"></view>
      </view>
      <view class = "count">
        <view class ="right">
          <text>正确</text>
          <text>4</text>
        </view>
        <view class = "wrong">
          <text>错误</text>
          <text>7</text>
        </view>
        <view class ="undone"> 
          <text>未答</text>
          <text>10</text>
        </view>
      </view>
    </view>
  </view>
  <view class="downer">

  </view>
</view>