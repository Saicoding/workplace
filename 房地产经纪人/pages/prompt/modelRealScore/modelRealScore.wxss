/* pages/prompt/modelRealScore/modelRealScore.wxss */

.mycontainer {
  display: block;
}

.upper {
  width: 100%;
  height: 650rpx;
  background: #6db7fe;
  align-content: center;
  border: 1px solid #6db7fe;
}

.upper-in {
  width: 700rpx;
  margin: 38rpx auto;
  height: 570rpx;
  background: white;
  border-radius: 20rpx;
  overflow-x:hidden;
}

.downer {
  width: 100%;
  height: 100%;
}

.time {
  padding-top: 20rpx;
}

.time-img {
  display: inline-flex;
  width: 50rpx;
  height: 50rpx;
  margin-left: 30rpx;
}

.time-text {
  display: inline-flex;
  margin-left: 20rpx;
  font-size: 35rpx;
  color: #0296f9;
  vertical-align: top;
}

.score {
  width: 100%;
  height: 300rpx;
}

.score-top {
  width: 100%;
  display: block;
  color: #0296f9;
  font-size: 180rpx;
  text-align: center;
}

.score-bottom {
  width: 100%;
  display: block;
  color: #0799f9;
  font-size: 45rpx;
  text-align: center;
}

.line {
  width: 100%;
  height: 80rpx;
}

.line-left, .line-right {
  display: inline-flex;
  height: 80rpx;
  width: 80rpx;
  border-radius: 80rpx;
  background: #0296f9;
}

.line-left{
  margin-left: -40rpx;
}
.line-right{
  margin-right:-40rpx;
}

.line-middle {
  display: inline-flex;
  width:620rpx;
  height:80rpx;
}

.line-middle-in{
  display: float;
  float:left;
  border-top:2px dashed #0296f9;
  width:100%;
  height:40rpx;
}
