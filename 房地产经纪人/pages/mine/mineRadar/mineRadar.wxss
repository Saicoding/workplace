.down {
}

.radarContainer {
  background-color: white;
  width: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
  position: relative;
  margin-top:10rpx;
}

.radarCanvas {
  clear: both;
  width: 700rpx;
  height: 600rpx;
  margin: 0rpx auto;
}

.loading-img{
  display: block;
  width: 118rpx;
  height:150rpx;
  margin:300rpx auto;
}

.time-icon,.time-text{
  display: float;
  float:left;
}

.changeType{
  width:100%;
  height: 90rpx;
  line-height: 90rpx;
  font-size: 30rpx;
  padding-right: 20rpx;
  border-bottom: 1px #cecece solid;
  box-shadow: rgb(211, 206, 206) 3rpx 3rpx 8rpx 1rpx ;
}

.time-icon{
  width:40rpx;
