.mycontainer{
  background: #f3f7f8;
}

.up{
  background: #0197f6;
  width:100%;
  height:350rpx;
  color:white;
  padding-top:30rpx;
}

.pic-container{
  width:200rpx;
  height:200rpx;
  border-radius: 200rpx;
  margin: 0 auto;
}

.pic{
  width:200rpx;
  height:200rpx;
  border-radius: 200rpx;
}

.nickname{
  font-size:50rpx;
  width:100%;
  text-align: center;
  margin-top:25rpx;
  letter-spacing: 8rpx;
}

.rate{
  border-top:25rpx solid #f3f7f8;
  padding-left:40rpx;
  padding-right:40rpx;
  background:white;
}


.jjr-block,.xl-block{
  height:100rpx;
  width:100%;
  border-bottom:1rpx solid #ebebeb;
  line-height: 100rpx;
}

.mytitle,.rate-img,.mytitle{
  display: float;
  float:left;
}

.rate-img{
  width: 50rpx;
  height:50rpx;
  margin-top:25rpx;
}

.mytitle{
  font-size:33rpx;
  width:548rpx;
  margin-left:20rpx;
}

.jie{
  display: block;
  width:100%;
  height: 0rpx;
  overflow: hidden;
}

.mytext,.right-img{
  display: float;
  float:left;
}

.block{
  height:90rpx;
  width:100%;
  vertical-align: middle;
  border-left:1rpx dashed #ebebeb;
  border-bottom: 1rpx dashed #ebebeb;
}

.right-img1{
  width:50rpx;
  height:50rpx;
  margin-top:25rpx;
}

.right-img{
  width:50rpx;
  height:35rpx;
  margin-top:28rpx;
}

.mytext{
  font-size:29rpx;
  width:545rpx;
  height:90rpx;
  margin-left:20rpx;
  padding-left:48rpx;
  line-height: 90rpx;
}

.bottom{
  border-top:20rpx solid #f3f7f8;
  border-bottom:10rpx solid #f3f7f8;
  background:white;
  font-size:33rpx;
  width:100%;
  height:100rpx;
  line-height: 100rpx;
}

.about,.logo-img-container{
  display: float;
  float:left;
}

.logo-img-container{
  width:40rpx;
