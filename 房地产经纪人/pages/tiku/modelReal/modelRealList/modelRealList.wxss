.item {
  display: flex;
  width: 100%;
  height: 120rpx;
  border-bottom: 1px dashed #eee;
  padding-left: 25rpx;
}

.loading-img {
  display: block;
  width: 118rpx;
  height:150rpx;
  margin:300rpx auto;
}


.title {
  height:120rpx;
  line-height: 120rpx;
  font-size: 28rpx;
  width: 630rpx;
  margin-left: 10rpx;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}

.score {
  width: 50rpx;
  height: 50rpx;
  line-height: 47rpx;
  margin-top: 35rpx;
  margin-left: 30rpx;
  font-size: 25rpx;
  text-align: center;
}

.select {
  background: rgb(223, 223, 223);
}
