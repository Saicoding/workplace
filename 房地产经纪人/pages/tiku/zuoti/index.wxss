/* pages/tiku/zuoti/index.wxss */
page{
  margin: 0px;
  padding:0px;
}

/******************** 提问 **********************/
.question {
  line-height: 1.5rem;
  width: 96%;
  margin: 0.5rem auto 1rem auto;
}

.question-icon{
  width:40rpx;
  height:40rpx;
  vertical-align:middle;
}

.question-block1{
  display: float;
  float:left;
  vertical-align:middle;
}

.qustion-num-text{
  display: float;
  float:left;
}

.qustion-select{
  vertical-align:middle;
  display: float;
  float:left;
}

.radio-group {
  width: 96%;
  padding: 1rem;
  margin: 1rem auto;
  font-size: 1rem;
  border-top: 1px solid #ccc;
}

.checkbox-group {
  width: 96%;
  padding: 1rem;
  margin: 1rem auto;
  font-size: 1rem;
  border-top: 1px solid #ccc;
}

.radio {
  width: 100%;
  font-size: 0.9rem;
  display: block;
  float: left;
  line-height: 1.5rem;
  margin: 0.8rem auto;
}

.checkbox {
  width: 100%;
  font-size: 0.9rem;
  display: block;
  float: left;
  line-height: 1.5rem;
  margin: 0.8rem auto;
}
/******************** 解析 **********************/
.jiexi_border{
  height: 80rpx;
  line-height: 120rpx;
  text-align: center;
}

.jiexi_border .line {
   display: inline-block;
   width: 240rpx;
   border-top: 1px solid #ccc ;
}
.jiexi_border .txt {
   color: #686868;
   margin:0 20rpx;
   font-size:25rpx;
   vertical-align: middle;
}

.log-list {
  display: flex;
  flex-direction: column;
  padding: 40rpx;
}
.aa{
  background-color: #666;
}

.jiexi {
  margin: 0 auto;
  text-align: center;
  clear: both;
  width: 80%;
  color: #0197f6;
  line-height: 2rem;
  height: 2rem;
}
/******************** 提交答案 **********************/
.submit_answer {
  width: 50%;
}

.jiexi view {
  color: #666;
  text-align: left;
  font-size: 0.8rem;
  line-height: 1.2rem;
  text-indent: 2em;
}

.bottom {
  display: inline-block;
  width: 100%;
  position: fixed;
  height: 2rem;
  bottom: 0;
  border-top: 1px solid#f3f3f3;
  padding-top:18rpx;
  padding-left:-30rpx;
  background-color:#fbf9f9;
}

.bottom-text{
  font-size: 35rpx;
  display: inline-block;
  margin-left:30rpx;
  padding-top:-50rpx;
}

.bottom-block{
 display: inline-block;
 margin-left:100rpx;
}

.bottom-img{
  width:30rpx;
  height:30rpx;
  display: inline-block;
}
/* 答案选项前面的图片 */
.answer-img{
  width:50rpx;
  height:50rpx;
  display: float;
  float:left;
}
/* 答案选项的文字 */
.answer-text{
  display: float;
  float:left;
  width:620rpx;
  height:50rpx;
  line-height: 45rpx;
  margin-left:30rpx;
}

.containers {
  width: 100%;
  height: 100%;
  margin: 0;
  padding: 0;
}

page {
  height: 100%;
}


.daan_dui {
  position: fixed;
  top: 3rem;
  width: 100%;
  height: 40%;
  background: url('data:image/png;base64,') no-repeat center;
  background-size: 40%;
}

.daan_cuo {
  position: fixed;
  top: 3rem;
  width: 100%;
  height: 40%;
  background: url('data:image/png;base64,') no-repeat center;
  background-size: 40%;
}
