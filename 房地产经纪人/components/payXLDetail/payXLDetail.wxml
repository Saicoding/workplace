<!--components/Dialog/dialog.wxml-->
<view hidden="{{!isShow}}" class="container-out" catchtap='stopBubbling'>
  <view class="container-in" style = "top:{{(windowHeight-720)/2}}rpx" >
    <image bindtap= "hideDialog" style="display:float;float:right;width:40rpx;height:40rpx;margin-right:15rpx;margin-top:20rpx;" src="/imgs/close.png" mode="aspectFit"></image>
    <view class="title">经纪人协理套餐</view>
    <view class="title" style="border-bottom:1rpx solid #d5d5d5;padding-bottom:35rpx;">冲刺班（2580元）</view>

    <view class="content">
      <view> <span style="font-weight:bold;color:#2ca8f8">|</span>      套餐包含内容：\r\n</view>
      <view>     <span style="font-weight:bold;">·</span> 全科目在线章节练习 \r\n</view>
      <view>     <span style="font-weight:bold;">·</span> 考前押题 \r\n</view>
      <view>     <span style="font-weight:bold;">·</span> 考前秘笈 \r\n</view>
      <view>     <span style="font-weight:bold;">·</span> 全科目网课 \r\n</view>
      <view>     <span style="font-weight:bold;">·</span> 考点解析 \r\n</view>
      <view style = "width:100%;height:50rpx;"></view>
      <view> <span style="font-weight:bold;color:#2ca8f8">|</span>      有效期：\r\n</view>
      <view>     <span style="font-weight:bold;">·</span> 自购买之日两年内有效 \r\n</view>
    </view>

    <view class="submit" bindtap="_submit">
      确认立即开通
    </view>

  </view>
</view>

<view wx:if="{{isShow}}" class="modal" bindtap="tapBlank">
</view>