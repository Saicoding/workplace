.container{
    background: #ffffff;
  width:94%;
  margin: 0 auto;
  box-shadow: rgb(211, 206, 206) 5rpx 5rpx 10rpx 3rpx ;
  border-radius: 10rpx;
}

.checkbox-group {
  width: 100%;
  font-size: 30rpx;
  margin-top: 50rpx;
}


.img-container {
  display: inline-flex;
  width: 50rpx;
}

.answer-block {
  width: 100%;
  font-size: 35rpx;
  padding-left: 30rpx;
  padding-top: 25rpx;
  padding-bottom: 25rpx;
  vertical-align: top;
}

.answer-img {
  width: 50rpx;
  height: 50rpx;
}

/* 答案选项的文字 */

.answer-text {
  display: inline-flex;
  font-size: 30rpx;
  margin-left:40rpx;
  line-height:50rpx;
  width: 543rpx;
  margin-top:-5rpx;
  vertical-align: top;
}
