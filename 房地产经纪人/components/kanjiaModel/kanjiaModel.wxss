/* components/errorRecovery/errorRecovery.wxss */
.container-in{
  width:100%;
  height:100%;
  position: absolute;
  top:0rpx;
  z-index: 100;
  background: url("data:image/png;base64,") no-repeat center 100rpx;
  background-size:80%;



}

.text1,.text2{
  color:white;
  text-align: center;
  width:100%;
}

.text1{
  font-size:45rpx;
  margin-top:140rpx;
}

.text2{
  height:120rpx;
  line-height: 120rpx;
  width:370rpx;
  margin:0 auto;
  font-size:75rpx;
  margin-top:45rpx;
  letter-spacing: 5rpx;
  background: #7a462f;
  border-radius: 10rpx;
}

.btn1,.btn2{
  width:60%;
  height:110rpx;
  margin:0 auto;
}

.btn1{
  margin-top:25rpx;
  opacity: 0;
}

.btn2{
  margin-top:38rpx;
}

.close{
  width:100rpx;
  height:100rpx;
  margin-left:563rpx;
  margin-top:130rpx;
}

.modal{
  position: absolute;
  top:0rpx;
  left:0rpx;
  width:100%;
  height:100%;
  filter:Alpha(Opacity=60);
  background-color:#1F1F1F;
  opacity: 0.5;
  z-index: 99;
}
